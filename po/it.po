#: ../usr/lib/debian-plymouth-manager/dpm.py:50
#: ../usr/lib/debian-plymouth-manager/main.py:30
#: ../usr/share/kde4/services/kcm_debian_plymouth_manager.desktop.in.h:1
#: ../usr/share/applications/debian-plymouth-manager.desktop.in.h:1
msgid "Debian Plymouth Manager"
msgstr "Debian Plymouth Manager"

#: ../usr/lib/debian-plymouth-manager/dpm.py:52
msgid "Plymouth"
msgstr "Plymouth"

#: ../usr/lib/debian-plymouth-manager/dpm.py:53
msgid "Themes"
msgstr "Temi"

#: ../usr/lib/debian-plymouth-manager/dpm.py:54
msgid "Grub"
msgstr "Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:61
msgid "None: no plymouth splash"
msgstr "Nessuno: nessun tema di plymouth"

#: ../usr/lib/debian-plymouth-manager/dpm.py:104
msgid "Set Plymouth Theme"
msgstr "Imposta tema Plymouth"

#: ../usr/lib/debian-plymouth-manager/dpm.py:105
#: ../usr/lib/debian-plymouth-manager/dpm.py:255
msgid "Preview"
msgstr "Anteprima"

#: ../usr/lib/debian-plymouth-manager/dpm.py:111
#: ../usr/lib/debian-plymouth-manager/dpm.py:162
msgid "Installed Themes"
msgstr "Temi installati"

#: ../usr/lib/debian-plymouth-manager/dpm.py:130
msgid "Resolutions"
msgstr "Risoluzioni"

#: ../usr/lib/debian-plymouth-manager/dpm.py:150
msgid "Install Theme"
msgstr "Installa tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:151
msgid "Remove Theme"
msgstr "Rimuovi tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:157
msgid "Available Themes"
msgstr "Temi disponibili"

#: ../usr/lib/debian-plymouth-manager/dpm.py:175
msgid "Set Grub Resolution"
msgstr "Imposta risoluzione Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:181
msgid "Grub Resolutions"
msgstr "Risoluzioni Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:256
#, python-format
msgid ""
"You must save before you can preview:\n"
"\n"
"Theme: %(theme)s\n"
"Resolution: %(res)s"
msgstr ""
"Prima di poter visualizzare un'anteprima è necessario salvare:\n"
"\n"
"Tema: %(theme)s\n"
"Risoluzione: %(res)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:267
#, python-format
msgid "Save setting: %(theme)s (%(res)s)"
msgstr "Salva impostazioni: %(theme)s (%(res)s)"

#: ../usr/lib/debian-plymouth-manager/dpm.py:291
#, python-format
msgid "Done saving settings: %(theme)s (%(res)s)"
msgstr "Salvataggio impostazioni completato: %(theme)s (%(res)s)"

#: ../usr/lib/debian-plymouth-manager/dpm.py:293
msgid "Save settings"
msgstr "Salva impostazioni"

#: ../usr/lib/debian-plymouth-manager/dpm.py:294
#, python-format
msgid ""
"Theme: %(theme)s\n"
"Resolution: %(res)s\n"
"\n"
"Done"
msgstr ""
"Tema: %(theme)s\n"
"Risoluzione: %(res)s\n"
"\n"
"Completato"

#: ../usr/lib/debian-plymouth-manager/dpm.py:312
msgid "install"
msgstr "installazione"

#: ../usr/lib/debian-plymouth-manager/dpm.py:315
msgid "Install theme"
msgstr "Installa tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:315
#, python-format
msgid ""
"Continue installing theme:\n"
"%(theme)s"
msgstr ""
"Continua e installa il tema:\n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:319
#, python-format
msgid "Start installing theme: %(theme)s"
msgstr "Avvia l'installazione del tema: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:331
#, python-format
msgid "User cancel install theme: %(theme)s"
msgstr "Installazione del tema annullata dall'utente: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:333
#: ../usr/lib/debian-plymouth-manager/dpm.py:360
#: ../usr/lib/debian-plymouth-manager/dpm.py:384
#, python-format
msgid "%(act1)s%(act2)s theme"
msgstr "%(act1)s%(act2)s tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:334
#, python-format
msgid ""
"The package cannot be installed: %(pck)s\n"
"Try apt instead"
msgstr ""
"Non è possibile installare il seguente pacchetto: %(pck)s\n"
"Provare ad installarlo con apt"

#: ../usr/lib/debian-plymouth-manager/dpm.py:339
msgid "remove"
msgstr "rimozione"

#: ../usr/lib/debian-plymouth-manager/dpm.py:342
msgid "Remove theme"
msgstr "Rimuovi tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:342
#, python-format
msgid ""
"Continue removing theme:\n"
"%(theme)s"
msgstr ""
"Continua e rimuovi il tema:\n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:348
#, python-format
msgid "Start removing theme: %(theme)s"
msgstr "Avvia la rimozione del tema: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:358
#, python-format
msgid "User cancel remove theme: %(theme)s"
msgstr "Rimozione del tema annullata dall'utente: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:361
#, python-format
msgid ""
"The package cannot be removed: %(pck)s\n"
"It is part of a meta package.\n"
"Try apt instead"
msgstr ""
"Impossibile rimuovere il seguente pacchetto: %(pck)s\n"
"È parte di un metapacchetto.\n"
"Provare a rimuoverlo usando apt"

#: ../usr/lib/debian-plymouth-manager/dpm.py:386
#, python-format
msgid ""
"Could not %(action)s theme:\n"
"%(theme)s\n"
"Try apt instead.\n"
"\n"
"Error message:\n"
"%(err)s"
msgstr ""
"Impossibile completare la %(action)s del tema:\n"
"%(theme)s\n"
"Provare ad usare apt.\n"
"\n"
"Messaggio di errore:\n"
"%(err)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:388
#, python-format
msgid ""
"%(action)s successfully of:\n"
"%(pck)s"
msgstr ""
"%(action)s completata con successo per:\n"
"%(pck)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:400
#, python-format
msgid "Save grub resolution: %(res)s"
msgstr "Salva risoluzione grub: %(res)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:419
msgid "Grub resolution"
msgstr "Risoluzione Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:420
#, python-format
msgid "Grub resolution saved: %(res)s"
msgstr "Risoluzione Grub salvata: %(res)s"

#: ../usr/lib/debian-plymouth-manager/grub.py:47
#: ../usr/lib/debian-plymouth-manager/plymouth.py:136
msgid "Neither grub nor burg found in /etc/default"
msgstr "Né grub né burg sono stati trovati in /etc/default"

#: ../usr/lib/debian-plymouth-manager/grub.py:72
msgid "No grub or burg found"
msgstr "Grub o burg non sono stati rilevati"

#: ../usr/lib/debian-plymouth-manager/main.py:31
msgid ""
"Debian Plymouth Manager cannot be started in a live environment\n"
"You can use the --force argument to start DPM in a live environment"
msgstr ""
"DPM non può essere eseguito in un live environment\n"
"\n"
"Per forzare l'avvio, usare l'argomento --force"

#: ../usr/lib/debian-plymouth-manager/main.py:81
msgid "Unexpected error"
msgstr "Errore inatteso"

#: ../usr/lib/debian-plymouth-manager/main.py:82
msgid ""
"Debian Plymouth Manager has failed with the following unexpected error.\n"
"Please submit a bug report!"
msgstr ""
"DDM riporta il seguente messaggio di errore inatteso. Si consiglia di "
"inviare una segnalazione del bug!"

#: ../usr/lib/debian-plymouth-manager/execapt.py:26
#, python-format
msgid "Error returned: %(err)s"
msgstr "Errore restituito: %(err)s"
