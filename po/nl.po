msgstr "Geen Plymouth splash"

#: ../usr/lib/debian-plymouth-manager/dpm.py:104
msgid "Set Plymouth Theme"
msgstr "Thema instellen"

#: ../usr/lib/debian-plymouth-manager/dpm.py:105
#: ../usr/lib/debian-plymouth-manager/dpm.py:255
msgid "Preview"
msgstr "Voorbeeld"

#: ../usr/lib/debian-plymouth-manager/dpm.py:111
#: ../usr/lib/debian-plymouth-manager/dpm.py:162
msgid "Installed Themes"
msgstr "Geïnstalleerde thema's"

#: ../usr/lib/debian-plymouth-manager/dpm.py:130
msgid "Resolutions"
msgstr "Resoluties"

#: ../usr/lib/debian-plymouth-manager/dpm.py:150
msgid "Install Theme"
msgstr "Installeer thema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:151
msgid "Remove Theme"
msgstr "Thema verwijderen"

#: ../usr/lib/debian-plymouth-manager/dpm.py:157
msgid "Available Themes"
msgstr "Beschikbare thema's"

#: ../usr/lib/debian-plymouth-manager/dpm.py:175
msgid "Set Grub Resolution"
msgstr "Resolutie instellen"

#: ../usr/lib/debian-plymouth-manager/dpm.py:181
msgid "Grub Resolutions"
msgstr "GRUB resoluties"

#: ../usr/lib/debian-plymouth-manager/dpm.py:256
#, python-format
msgid ""
"You must save before you can preview:\n"
"\n"
"Theme: %(theme)s\n"
"Resolution: %(res)s"
msgstr ""
"U dient te bewaren voordat u het voorbeeld kunt zien:\n"
"\n"
"Thema: %(theme)s\n"
"Resolutie: %(res)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:267
#, python-format
msgid "Save setting: %(theme)s (%(res)s)"
msgstr "Instellingen opslaan: %(theme)s (%(res)s)"

#: ../usr/lib/debian-plymouth-manager/dpm.py:291
#, python-format
msgid "Done saving settings: %(theme)s (%(res)s)"
msgstr "Instellingen bewaard: %(theme)s (%(res)s)"

#: ../usr/lib/debian-plymouth-manager/dpm.py:293
msgid "Save settings"
msgstr "Instellingen opslaan"

#: ../usr/lib/debian-plymouth-manager/dpm.py:294
#, python-format
msgid ""
"Theme: %(theme)s\n"
"Resolution: %(res)s\n"
"\n"
"Done"
msgstr ""
"Thema: %(theme)s\n"
"Resolutie: %(res)s\n"
"\n"
"Done"

#: ../usr/lib/debian-plymouth-manager/dpm.py:312
msgid "install"
msgstr "installeren"

#: ../usr/lib/debian-plymouth-manager/dpm.py:315
msgid "Install theme"
msgstr "Thema installeren"

#: ../usr/lib/debian-plymouth-manager/dpm.py:315
#, python-format
msgid ""
"Continue installing theme:\n"
"%(theme)s"
msgstr ""
"Installeer thema:\n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:319
#, python-format
msgid "Start installing theme: %(theme)s"
msgstr "Start installatie thema: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:331
#, python-format
msgid "User cancel install theme: %(theme)s"
msgstr "Gebruiker heeft installatie afgebroken: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:333
#: ../usr/lib/debian-plymouth-manager/dpm.py:360
#: ../usr/lib/debian-plymouth-manager/dpm.py:384
#, python-format
msgid "%(act1)s%(act2)s theme"
msgstr "%(act1)s%(act2)s thema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:334
#, python-format
msgid ""
"The package cannot be installed: %(pck)s\n"
"Try apt instead"
msgstr ""
"Dit pakket kan niet geïnstalleerd worden: %(pck)s\n"
"Probeer apt in plaats hiervan"

#: ../usr/lib/debian-plymouth-manager/dpm.py:339
msgid "remove"
msgstr "verwijderen"

#: ../usr/lib/debian-plymouth-manager/dpm.py:342
msgid "Remove theme"
msgstr "Thema verwijderen"

#: ../usr/lib/debian-plymouth-manager/dpm.py:342
#, python-format
msgid ""
"Continue removing theme:\n"
"%(theme)s"
msgstr ""
"Verwijder thema:\n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:348
#, python-format
msgid "Start removing theme: %(theme)s"
msgstr "Start verwijdering thema: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:358
#, python-format
msgid "User cancel remove theme: %(theme)s"
msgstr "Gebruiker heeft verwijderen afgebroken: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:361
#, python-format
msgid ""
"The package cannot be removed: %(pck)s\n"
"It is part of a meta package.\n"
"Try apt instead"
msgstr ""
"Dit pakket kan niet verwijderd worden: %(pck)s\n"
"Het is deel van een metapakket.\n"
"Probeer apt in plaats hiervan"

#: ../usr/lib/debian-plymouth-manager/dpm.py:386
#, python-format
msgid ""
"Could not %(action)s theme:\n"
"%(theme)s\n"
"Try apt instead.\n"
"\n"
"Error message:\n"
"%(err)s"
msgstr ""
"Kon thema niet %(action)s:\n"
"%(theme)s\n"
"Probeer apt in plaats hiervan.\n"
"\n"
"Foutbericht:\n"
"%(err)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:388
#, python-format
msgid ""
"%(action)s successfully of:\n"
"%(pck)s"
msgstr ""
"%(action)s succesvol van:\n"
"%(pck)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:400
#, python-format
msgid "Save grub resolution: %(res)s"
msgstr "Bewaar grub resolutie: %(res)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:419
msgid "Grub resolution"
msgstr "Grubresolutie"

#: ../usr/lib/debian-plymouth-manager/dpm.py:420
#, python-format
msgid "Grub resolution saved: %(res)s"
msgstr "Grub resolutie bewaard: %(res)s"

#: ../usr/lib/debian-plymouth-manager/grub.py:47
#: ../usr/lib/debian-plymouth-manager/plymouth.py:136
msgid "Neither grub nor burg found in /etc/default"
msgstr "Zowel grub als burg zijn niet in /etc/default gevonden"

#: ../usr/lib/debian-plymouth-manager/grub.py:72
msgid "No grub or burg found"
msgstr "Grub of burg niet gevonden"

#: ../usr/lib/debian-plymouth-manager/main.py:31
msgid ""
"Debian Plymouth Manager cannot be started in a live environment\n"
"You can use the --force argument to start DPM in a live environment"
msgstr ""
"DPM kan niet in een live omgeving gestart worden\n"
"Om toch te starten, gebruik het --force argument"

#: ../usr/lib/debian-plymouth-manager/main.py:81
msgid "Unexpected error"
msgstr "Onverwachte fout"

#: ../usr/lib/debian-plymouth-manager/main.py:82
msgid ""
"Debian Plymouth Manager has failed with the following unexpected error.\n"
"Please submit a bug report!"
msgstr ""
"Debian Plymouth Manager kan niet starten om de volgende fout.\n"
"Gelieve een bug verslag in te dienen!"

#: ../usr/lib/debian-plymouth-manager/execapt.py:26
#, python-format
msgid "Error returned: %(err)s"
msgstr "Fout geretourneerd: %(err)s"
