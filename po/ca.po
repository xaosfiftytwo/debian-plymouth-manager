msgstr "Temes"

#: ../usr/lib/debian-plymouth-manager/dpm.py:54
msgid "Grub"
msgstr "Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:61
msgid "None: no plymouth splash"
msgstr "Cap: No hi ha flaix de Plymouth"

#: ../usr/lib/debian-plymouth-manager/dpm.py:103
msgid "Set Plymouth Theme"
msgstr "Defineix el Tema Plymouth"

#: ../usr/lib/debian-plymouth-manager/dpm.py:104
#: ../usr/lib/debian-plymouth-manager/dpm.py:254
msgid "Preview"
msgstr "Previsualitza"

#: ../usr/lib/debian-plymouth-manager/dpm.py:110
#: ../usr/lib/debian-plymouth-manager/dpm.py:161
msgid "Installed Themes"
msgstr "Temes Instal·lats"

#: ../usr/lib/debian-plymouth-manager/dpm.py:129
msgid "Resolutions"
msgstr "Resolucions"

#: ../usr/lib/debian-plymouth-manager/dpm.py:149
msgid "Install Theme"
msgstr "Instal·la un tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:150
msgid "Remove Theme"
msgstr "Elimina un tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:156
msgid "Available Themes"
msgstr "Temes disponibles"

#: ../usr/lib/debian-plymouth-manager/dpm.py:174
msgid "Set Grub Resolution"
msgstr "Defineix la resolució del Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:180
msgid "Grub Resolutions"
msgstr "Resolucions del GRUB:"

#: ../usr/lib/debian-plymouth-manager/dpm.py:255
#, python-format
msgid ""
"You must save before you can preview:\n"
"\n"
"Theme: %(theme)s\n"
"Resolution: %(res)s"
msgstr ""
"Cal que deseu abans de previsualitzar:\n"
"\n"
"Tema: %(theme)s\n"
"Resolució: %(res)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:266
#, python-format
msgid "Save setting: %(theme)s (%(res)s)"
msgstr "Desa els paràmetres: %(theme)s (%(res)s)"

#: ../usr/lib/debian-plymouth-manager/dpm.py:292
#, python-format
msgid "Done saving settings: %(theme)s (%(res)s)"
msgstr "Paràmetres desats: %(theme)s (%(res)s)"

#: ../usr/lib/debian-plymouth-manager/dpm.py:294
msgid "Save settings"
msgstr "Desa els paràmetres"

#: ../usr/lib/debian-plymouth-manager/dpm.py:295
#, python-format
msgid ""
"Theme: %(theme)s\n"
"Resolution: %(res)s\n"
"\n"
"Done"
msgstr ""
"Tema: %(theme)s\n"
"Resolució: %(res)s\n"
"\n"
"Fet."

#: ../usr/lib/debian-plymouth-manager/dpm.py:313
msgid "install"
msgstr "Instal·la"

#: ../usr/lib/debian-plymouth-manager/dpm.py:316
msgid "Install theme"
msgstr "Instal·la un tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:316
#, python-format
msgid ""
"Continue installing theme:\n"
"%(theme)s"
msgstr ""
"Continua instal·lant el tema:\n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:320
#, python-format
msgid "Start installing theme: %(theme)s"
msgstr "Comença la instal·lació del tema: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:332
#, python-format
msgid "User cancel install theme: %(theme)s"
msgstr "Instal·lació del tema cancel·lada per l'usuari: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:334
#: ../usr/lib/debian-plymouth-manager/dpm.py:361
#: ../usr/lib/debian-plymouth-manager/dpm.py:385
#, python-format
msgid "%(act1)s%(act2)s theme"
msgstr "Tema %(act1)s%(act2)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:335
#, python-format
msgid ""
"The package cannot be installed: %(pck)s\n"
"Try apt instead"
msgstr ""
"No es pot instal·lar el paquet: %(pck)s\n"
"Proveu de fer-ho amb APT"

#: ../usr/lib/debian-plymouth-manager/dpm.py:340
msgid "remove"
msgstr "elimina"

#: ../usr/lib/debian-plymouth-manager/dpm.py:343
msgid "Remove theme"
msgstr "Elimina el tema"

#: ../usr/lib/debian-plymouth-manager/dpm.py:343
#, python-format
msgid ""
"Continue removing theme:\n"
"%(theme)s"
msgstr ""
"Continua eliminant el tema: \n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:349
#, python-format
msgid "Start removing theme: %(theme)s"
msgstr "Comença a eliminar el tema: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:359
#, python-format
msgid "User cancel remove theme: %(theme)s"
msgstr "Eliminació del tema cancel·lada per l'usuari: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:362
#, python-format
msgid ""
"The package cannot be removed: %(pck)s\n"
"It is part of a meta package.\n"
"Try apt instead"
msgstr ""
"No es pot eliminar el paquet: %(pck)s\n"
"És part d'un meta-paquet.\n"
"Proveu de fer-ho amb APT."

#: ../usr/lib/debian-plymouth-manager/dpm.py:387
#, python-format
msgid ""
"Could not %(action)s theme:\n"
"%(theme)s\n"
"Try apt instead.\n"
"\n"
"Error message:\n"
"%(err)s"
msgstr ""
"No s'ha pogut %(action)s al tema:\n"
"%(theme)s\n"
"\n"
"Missatge d'error:\n"
"%(err)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:389
#, python-format
msgid ""
"%(action)s successfully of:\n"
"%(pck)s"
msgstr ""
"%(action)s amb èxit de:\n"
"'%(pck)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:401
#, python-format
msgid "Save grub resolution: %(res)s"
msgstr "Desa la resolució del Grub: %(res)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:420
msgid "Grub resolution"
msgstr "Resolució del Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:421
#, python-format
msgid "Grub resolution saved: %(res)s"
msgstr "Desada la resolució del Grub: %(res)s"

#: ../usr/lib/debian-plymouth-manager/grub.py:46
#: ../usr/lib/debian-plymouth-manager/plymouth.py:136
msgid "Neither grub nor burg found in /etc/default"
msgstr "No s'ha trobat ni grub ni burg a /etc/default"

#: ../usr/lib/debian-plymouth-manager/grub.py:71
msgid "No grub or burg found"
msgstr "No s'ha trobat el Grub ni Burg"

#: ../usr/lib/debian-plymouth-manager/main.py:31
msgid ""
"Debian Plymouth Manager cannot be started in a live environment\n"
"You can use the --force argument to start DPM in a live environment"
msgstr ""
"Debian Plymouth Manager no es pot engegar en un entorn autònom\n"
"Podeu usar l'argument --force per engegar DPM en l'entorn autònom."

#: ../usr/lib/debian-plymouth-manager/main.py:81
msgid "Unexpected error"
msgstr "Error inesperat "

#: ../usr/lib/debian-plymouth-manager/main.py:82
msgid ""
"Debian Plymouth Manager has failed with the following unexpected error.\n"
"Please submit a bug report!"
msgstr ""
"Debian Plymouth Manager ha fallat amb aquest error inesperat.\n"
"Si us plau, envieu un informe d'errada!"

#: ../usr/lib/debian-plymouth-manager/execapt.py:26
#, python-format
msgid "Error returned: %(err)s"
msgstr "Ha retornat l'error: %(err)s"
