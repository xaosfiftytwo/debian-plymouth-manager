#: ../usr/lib/debian-plymouth-manager/dpm.py:338
#, python-format
msgid ""
"The package cannot be installed: %(pck)s\n"
"Try apt instead"
msgstr ""
"Το πακέτο δεν μπορεί να εγκατασταθεί: %(pck)s\n"
"Δοκιμάστε με το APT"

#: ../usr/lib/debian-plymouth-manager/dpm.py:343
msgid "remove"
msgstr "αφαίρεση"

#: ../usr/lib/debian-plymouth-manager/dpm.py:346
msgid "Remove theme"
msgstr "Αφαίρεση θέματος"

#: ../usr/lib/debian-plymouth-manager/dpm.py:346
#, python-format
msgid ""
"Continue removing theme:\n"
"%(theme)s"
msgstr ""
"Συνέχεια αφαίρεσης θέματος:\n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:352
#, python-format
msgid "Start removing theme: %(theme)s"
msgstr "Έναρξη αφαίρεσης θέματος: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:362
#, python-format
msgid "User cancel remove theme: %(theme)s"
msgstr "Ακύρωση αφαίρεσης θέματος από τον χρήστη: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:365
#, python-format
msgid ""
"The package cannot be removed: %(pck)s\n"
"It is part of a meta package.\n"
"Try apt instead"
msgstr ""
"Το πακέτο δεν μπορεί να αφαιρεθεί: %(pck)s\n"
"Είναι μέρος τού μετα-πακέτου.\n"
"Δοκιμάστε το apt"

#: ../usr/lib/debian-plymouth-manager/dpm.py:390
#, python-format
msgid ""
"Could not %(action)s theme:\n"
"%(theme)s\n"
"Try apt instead.\n"
"\n"
"Error message:\n"
"%(err)s"
msgstr ""
"Αδυναμία %(action)s θέμα:\n"
"%(theme)s\n"
"Ξαναδοκιμάστε με το apt.\n"
"\n"
"Μύνημα λάθους:\n"
"%(err)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:392
#, python-format
msgid ""
"%(action)s successfully of:\n"
"%(pck)s"
msgstr ""
"%(action)s επιτυχώς των:\n"
"%(pck)s"

