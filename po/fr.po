msgid "%(act1)s%(act2)s theme"
msgstr "%(act1)s%(act2)s thème"

#: ../usr/lib/debian-plymouth-manager/dpm.py:334
#, python-format
msgid ""
"The package cannot be installed: %(pck)s\n"
"Try apt instead"
msgstr ""
"Le paquet ne peut être installé: %(pck)s\n"
"Essayer plutôt avec apt"

#: ../usr/lib/debian-plymouth-manager/dpm.py:339
msgid "remove"
msgstr "Supprimer"

#: ../usr/lib/debian-plymouth-manager/dpm.py:342
msgid "Remove theme"
msgstr "Supprimer le thème"

#: ../usr/lib/debian-plymouth-manager/dpm.py:342
#, python-format
msgid ""
"Continue removing theme:\n"
"%(theme)s"
msgstr ""
"Continuer à supprimer le thème:\n"
"%(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:348
#, python-format
msgid "Start removing theme: %(theme)s"
msgstr "Démarrage de la suppression du thème: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:358
#, python-format
msgid "User cancel remove theme: %(theme)s"
msgstr "Abandon par l'utilisateur de la suppression du thème: %(theme)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:361
#, python-format
msgid ""
"The package cannot be removed: %(pck)s\n"
"It is part of a meta package.\n"
"Try apt instead"
msgstr ""
"Le paquet ne peut être supprimé: %(pck)s\n"
"il fait partie d'un meta-paquet.\n"
"Essayer plutôt avec apt"

#: ../usr/lib/debian-plymouth-manager/dpm.py:386
#, python-format
msgid ""
"Could not %(action)s theme:\n"
"%(theme)s\n"
"Try apt instead.\n"
"\n"
"Error message:\n"
"%(err)s"
msgstr ""
"Ne peut %(action)s le thème:\n"
"%(theme)s\n"
"Essayer plutôt avec apt.\n"
"\n"
"Message d'erreur:\n"
"%(err)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:388
#, python-format
msgid ""
"%(action)s successfully of:\n"
"%(pck)s"
msgstr ""
"Succès %(action)s :\n"
"%(pck)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:400
#, python-format
msgid "Save grub resolution: %(res)s"
msgstr "Enregistrer la résolution grub: %(res)s"

#: ../usr/lib/debian-plymouth-manager/dpm.py:419
msgid "Grub resolution"
msgstr "Résolution Grub"

#: ../usr/lib/debian-plymouth-manager/dpm.py:420
#, python-format
msgid "Grub resolution saved: %(res)s"
msgstr "Résolution Grub enregistrée: %(res)s"

